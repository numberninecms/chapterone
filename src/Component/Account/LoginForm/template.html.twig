{% if app.user %}
    <div class="mb-3">
        You are logged in as {{ app.user.username }}, <a href="{{ path('numbernine_logout') }}">Logout</a>
    </div>
{% else %}
    {{ form_start(form) }}
    {% if error %}
        <div class="alert-warning my-5" role="alert">
            <p class="font-bold">{{ error.messageKey|trans(error.messageData, 'security') }}</p>
        </div>
    {% endif %}
    {{ form_end(form) }}
{% endif %}
